{
  "manifest_version": 3,
  "name": "Nestify",
  "version": "1.0",
  "description": "Grow your virtual bird ecosystem while completing focus sessions to gamify your productivity habits",
  "options_ui": {
    "page": "src/pages/options/index.html"
  },
  "action": {
    "default_popup": "src/pages/popup/index.html",
    "default_icon": {
      "32": "icon-32.png"
    }
  },
  "icons": {
    "128": "icon-128.png"
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "identity",
    "declarativeNetRequest"
  ],
  "host_permissions": [
    "\u003Call_urls\u003E"
  ],
  "oauth2": {
    "client_id": "651949198469-3sea82uhvhqdrsnve229vub4ns43qoii.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*",
        "<all_urls>"
      ],
      "js": [
        "src/pages/content/index.tsx"
      ],
      "css": [
        "contentStyle.css"
      ]
    },
    {
      "matches": ["<all_urls>"],
      "js": ["src/mellowtel/mellowtel.js"],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "devtools_page": "src/pages/devtools/index.html",
  "web_accessible_resources": [
    {
      "resources": [
        "contentStyle.css",
        "icon-128.png",
        "icon-32.png"
      ],
      "matches": [
        "https://*/*",
        "http://*/*"
      ]
    }
  ],
  "key": ""
}
